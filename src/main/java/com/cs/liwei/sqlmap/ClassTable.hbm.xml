<?xml version="1.0" encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  
  
<hibernate-mapping package="com.cs.liwei.pojo">  
    <class name="ClassTable" table="class">  
    <!-- pojo类 与字段对应  -->
        <id name="className" column="className" type="java.lang.String">  
            <generator class="native"></generator> 
        </id>  
        <property name="professionNo" column="professionNo" type="java.lang.Integer" />  
        <property name="assisantNo" column="assisantNo" type="java.lang.Integer" />  
        <property name="stuTotal" column="stuTotal" type="java.lang.Integer" /> 
    </class>  
</hibernate-mapping> 